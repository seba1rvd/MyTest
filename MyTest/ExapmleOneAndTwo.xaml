<Window x:Class="MyTest.ExapmleOneAndTwo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyTest"
        mc:Ignorable="d"
        Title="Примеры задач" Height="800" Width="1200"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid>
            <Button x:Name="OneExample" Click="OneExample_Click"  Grid.Row="0" Content="1" Width="25" Height="25" HorizontalAlignment="Left" Margin="15" />
            <Button x:Name="TwoExaple" Click="TwoExaple_Click" Grid.Row="0" Content="2" Width="25" Height="25" HorizontalAlignment="Left" Margin="55 0 0 0" />
        </Grid>
        <Grid Grid.Row="1">
            <Frame x:Name="MainContentFrame" Margin="10 0 10 15" NavigationUIVisibility="Hidden"/>
        </Grid>
    </Grid>
</Window>
